{
    "manifest_version": 3,
    "name": "Mi Nueva Pestaña Personalizada",
    "version": "1.0",
    "description": "Reemplaza la página de nueva pestaña con contenido personalizado.",
    
    "permissions": [
        "storage"
    ],
    
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    
    "options_page": "options.html",

    "action": {
        "default_popup": "newtab.html"
    }
}
