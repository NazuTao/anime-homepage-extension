{
    "manifest_version": 3,
    "name": "Custom Home page for chrome",
    "version": "2.0",
    "description": "Permite personalizar las nuevas pesta√±as del navegador con imagenes y enlaces.",
    
    "permissions": [
        "storage"
    ],
    
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "action": {
        "default_popup": "newtab.html"
    },

    "icons": {
        "128": "images/icon.png"
    }
}