{
    "manifest_version": 3,
    "name": "Custom Home page for chrome",
    "version": "1.1",
    "description": "Permite personalizar las nuevas pesta√±as de chrome con imagenes y enlaces.",
    
    "permissions": [
        "storage"
    ],
    
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    
    "options_page": "options.html",

    "action": {
        "default_popup": "newtab.html"
    }
}
